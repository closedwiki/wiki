unless( -f "pub/_kupu/kupucnf.html" ) {

chdir('pub/_kupu');
print "Running Kupu installer...\n";
my $result = `make`;
if( $! ) {
    print STDERR "Error $! running 'make'\n$result\nKupu installation failed. See the Kupu documentation in pub/_kupu/doc/INSTALL.txt for details of how to complete installation of Kupu\n";
}

}
