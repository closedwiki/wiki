# CVSUSER is the sourceforge username used when using ssh authentication
# You will be prompted for the SSH password for that user.
export CVSUSER=mrjc
export PLUGINNAME=$1

export TWIKISTAGE=$TWIKIROOT/stage

# As of 14-Jan-03, Sourceforge's pserver is down. So, we can only check out ssh
export CVS_RSH=ssh
export CVSROOT=$CVSUSER@cvs.sourceforge.net:/cvsroot/twikiplugins

if [ ! "$PLUGINNAME" ]
then
   echo "usage: $0 pluginname"
   exit
fi

if [ ! -d "$TWIKISTAGE" ]
then
   print "Stage directory '$TWIKISTAGE' does not exist"
   exit
fi
