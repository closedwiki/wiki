%TMPL:INCLUDE{"twiki"}%
%TMPL:P{"htmldoctype"}%
<head>
 %TMPL:P{"headedit"}%
 %TMPL:P{"css"}%
</head>
<body>
#PageTop 
<div id="topsection"> %TMPL:P{"searchbox"}% </div>
<div id="content">
---+ Search results
%SPLIT%Search: <b> %SEARCHSTRING% </b>
<p />
%SPLIT%<table width="100%" border="0" cellpadding="0" cellspacing="4">
<tr bgcolor="%WEBBGCOLOR%">
  <td width="50%">
    <b>Topics in <nop>%WEB% web:</b>
  </td><td width="30%">
    <b>Changed:</b> now %GMTIME{"$hour:$min"}% GMT
  </td><td width="20%">
    <b>Changed by:</b>
  </td>
</tr>
</table>
%REPEAT%<table width="100%" border="0" cellspacing="0" cellpadding="0">
<tr>
  <td width="50%">
    <a href="%SCRIPTURLPATH%/view%SCRIPTSUFFIX%/%WEB%/%TOPICNAME%"><b>%TOPICNAME%</b></a>
    <b><font color="red">%LOCKED%</font><b>
  </td><td width="30%">
    <a href="%SCRIPTURLPATH%/rdiff%SCRIPTSUFFIX%/%WEB%/%TOPICNAME%">%TIME%</a> - %REVISION%
  </td><td width="20%">
    %AUTHOR%
  </td>
</tr>
<tr>
  <td colspan="2" width="80%">
    <font size="-1">
    %TEXTHEAD% </font>
  </td><td width="20%">
    &nbsp;
  </td>
</tr></table>
%REPEAT%<br clear="all" />
<p />%SPLIT%Number of topics: <b>%NTOPICS%</b>
<p />%SPLIT%
</div><!-- end content -->

<hr class="nocss">
<dl id="navigate">
	<dt><a name="Navigate"></a>Near:
	<dd> %WEBTOPICLIST%
	<dt><a name="%TMPL:P{"weblabel"}%s"></a>Far:
	<dd> %WIKIWEBLIST%
</dl>
<div id="logochar"> %LOGOCHAR% </div>
<span id="bugreport"> %TMPL:P{"bugreport"}% </span>
#PageBottom
</body>
</html>
%TMPL:DEF{"titleaction"}%(changes)%TMPL:END%
%TMPL:DEF{"webaction"}% %WEBTOPICLIST% %TMPL:END%
%TMPL:DEF{"topicaction"}% %TMPL:END%
