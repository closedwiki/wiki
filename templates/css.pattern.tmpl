<style type="text/css" media="all">
	/* Styles that are set using variables */
	.twikiToc li {
		background-image:url("%PUBURL%/%TWIKIWEB%/PatternSkin/bullet-down.gif");
	}	        
	.patternWebIndicator,
	.patternBookViewList .twikiTopRow {
		background-color:%WEBBGCOLOR%;
	}
	.twikiHelp,
	.patternAccessKeyInfo {
		background:url("%PUBURL%/%TWIKIWEB%/PatternSkin/info_bg.gif");
	}
	.patternLeftBarPersonal,
	.tipsOfTheDay {
		background:url("%PUBURL%/%TWIKIWEB%/PatternSkin/tips_bg.gif") #fff repeat-x;
	}
	.patternMain {
		background:url("%PUBURL%/%TWIKIWEB%/PatternSkin/page_bg.gif") #fff repeat-x;
	}
	.patternPrintPage .patternMain {
		background:#fff;
	}
	.twikiPageForm table {
		background-image:url("%PUBURL%/%TWIKIWEB%/PatternSkin/twikiPageForm_bg.gif");
	}
	.patternTopic .twikiTable th {
		background-image:url("%PUBURL%/%TWIKIWEB%/PatternSkin/twikiTable_header_bg.gif");
	}
	.patternLeftBarPersonal li {
		background-image:url("%PUBURL%/%TWIKIWEB%/PatternSkin/bullet-blue.gif");
	}
</style>
