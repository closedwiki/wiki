%TMPL:INCLUDE{"twiki"}%
%TMPL:INCLUDE{"rename"}%
%TMPL:DEF{"titleaction"}%%_{"(move attachment)"}% %TMPL:END%



%TMPL:DEF{"webaction"}% *%_{"Move file attachment"}%* %TMPL:END%



%TMPL:DEF{"bodystart"}%
<body class="patternNoViewPage patternRenamePage" onload="initPage()"><a name="PageTop"></a><div class="patternPage">%TMPL:END%



%TMPL:DEF{"topicactionbuttons"}%<input type="submit" class="twikiSubmit" value=' %_{"Move"}% ' /> %_{"or"}% %TMPL:P{"canceltopicaction"}%%TMPL:END%



%TMPL:DEF{"movewebhelp"}%%_{"To delete an attachment move it to web [_1], topic [_2].", "*%TRASHWEB%*", "*TrashAttachment*"}%%TMPL:END%



%TMPL:DEF{"namehelp"}%%_{"The new topic chosen must already exist."}%%TMPL:END%



%TMPL:DEF{"content"}%
<form name="rename" action="%SCRIPTURLPATH%/rename%SCRIPTSUFFIX%/%WEB%/%TOPIC%">
---+ %_{"Move or Delete File [_1]", "<code>%FILENAME%</code>"}%
<div class="patternVersatileTable">
<table>
<tr>
<th>%_{"Change from:"}%</th>
<td class="patternOldCol"><b><nop>%WEB%.%TOPIC%</b></td>
<td class="patternOldCol patternHelpCol"></td>
</tr>
<tr>
<th>%_{"To web:"}%</th>
<td class="patternMainCol"><select name="newweb"> %WEBLIST{"<option $marker value=$qname>$name</option>" webs="%TRASHWEB%,public" selection="%WEB%" separator=" "}% </select></td>
<td class="patternMainCol patternHelpCol">%TMPL:P{"movewebhelp"}%</td>
</tr>
<tr>
<th>%_{"Topic name:"}%</th>
<td class="patternMainCol"><input name="newtopic" value="%TOPIC%" /></td>
<td class="patternMainCol patternHelpCol">%TMPL:P{"namehelp"}%</td>
</tr>
</table>
</div>
<input type="hidden" name="attachment" value="%FILENAME%" />
<div class="twikiHelp">
*%_{"Note:"}%*
   * %_{"The attachment and any history will be moved to the new topic."}%
   * %_{"An error page will be shown if either of the topics are locked by another user."}%
   * %_{"Note that any URLs that refer to the attachment will be broken after this move."}%
</div>
%TMPL:P{"topicaction"}%
</form>%TMPL:END%
