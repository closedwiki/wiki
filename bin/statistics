BEGIN {
    # See the 'view' script for a description of what this does
    if( $ENV{"SCRIPT_FILENAME"} &&
        $ENV{"SCRIPT_FILENAME"} =~ /^(.+)\/[^\/]+$/ ) {
        chdir $1;
    }
    unshift @INC, '.';
    require 'setlib.cfg';
    if( $TWiki::useLocale ) {
        require locale;
        import locale ();
    }
}

use TWiki::UI;
TWiki::UI::run( "TWiki::UI::Statistics", "statistics" );
