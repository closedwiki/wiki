%TMPL:INCLUDE{"twiki"}%
%TMPL:P{"htmldoctype"}%
<head>
 %TMPL:P{"headview"}%
 %TMPL:P{"css"}%
</head>
<body bgcolor="%WEBBGCOLOR%">
#PageTop
<div id="topsection">%TMPL:P{"standardheader"}%</div>
<div id="midsection">
	<div id="content">
%SPLIT%Results of search for:<strong> %SEARCHSTRING% </strong>
%SPLIT%
<table width="100%" border="0" cellpadding="0" cellspacing="4">
<tr bgcolor="%WEBBGCOLOR%">
  <td width="50%">
    <strong>Topics in %WEB% web:</strong>
  </td><td width="30%">
    <strong>Changed:</strong> now %GMTIME{"$hour:$min"}% GMT
  </td><td width="20%">
    <strong>Changed by:</strong>
  </td>
</tr>
</table>
%REPEAT%<table width="100%" border="0" cellspacing="0" cellpadding="0">
<tr>
  <td width="50%">
    <a href="%SCRIPTURLPATH%/view%SCRIPTSUFFIX%/%WEB%/%TOPICNAME%"><strong>%TOPICNAME%</strong></a>
    <strong><font color="red">%LOCKED%</font><strong>
  </td><td width="30%">
    <a href="%SCRIPTURLPATH%/rdiff%SCRIPTSUFFIX%/%WEB%/%TOPICNAME%">%TIME%</a> - %REVISION%
  </td><td width="20%">
    %AUTHOR%
  </td>
</tr>
<tr>
  <td colspan="2" width="80%">
    <font size="-1">
    %TEXTHEAD% </font>
  </td><td width="20%">
    &nbsp;
  </td>
</tr></table>
	%REPEAT%
<p align="center">%SPLIT%Number of topics found: <strong> %NTOPICS% </strong>
%SPLIT%</p>
</div><!-- end content -->
<div id="navigation">
		<hr>
		<form name="main" action="%SCRIPTURLPATH%/view%SCRIPTSUFFIX%/%INTURLENCODE{"%WEB%"}%/">
			<!-- <form name="photonsearch" action="%SCRIPTURLPATH%/photonsearch%SCRIPTSUFFIX%/%INTURLENCODE{"%WEB%"}%" method="post"> -->
				<h4><a name="Navigate"></a>In this Section:</h4>
			<ul>
				<li>%WEBTOPICLIST%
			</ul>
		</form>
		<div id="zones">
			<hr>
			<h4><a name="%TMPL:P{"weblabel"}%s"></a>%TMPL:P{"weblabel"}%s:</h4>
			<ul>
				<li>%WIKIWEBLIST%
			</ul>
		</div><!-- end zones -->
	</div><!-- end Navigation -->
</div><!-- end midsection -->
<!-- begin "not in any section" -->
<div id="menu">
	<hr>	
	<a name="Menu"></a><h4>Menu</h4>
	<ul>
		%TMPL:P{"sep"}% <em>%TMPL:P{"titleaction"}%</em> <!-- %TMPL:P{"topicaction"}% -->
	</ul> 
</div><!-- end menu -->
<div id="bottsection">
	<hr>
	%WEBCOPYRIGHT%
</div><!-- end bottsection -->
#PageBottom
</body>
</html>
%TMPL:DEF{"titleaction"}%(search results)%TMPL:END%
%TMPL:DEF{"topicaction"}% %TMPL:END%
